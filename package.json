    "@remix-run/serve": "^1.1.1",
    "clsx": "^1.1.1",
    "contentful": "^9.1.5",
    "contentful-parsers": "^1.2.18",
    "dotenv": "^10.0.0",
    "firebase": "^9.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.1",
    "remixicon-react": "^1.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/react": "^17.0.24",
